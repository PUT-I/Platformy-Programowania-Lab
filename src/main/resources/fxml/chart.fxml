            <TextField fx:id="function" alignment="CENTER" editable="false" GridPane.columnIndex="2"
                       GridPane.rowIndex="1"/>
            <Slider fx:id="range" majorTickUnit="2.0" max="10.0" min="1.0" minorTickCount="1" showTickLabels="true"
                    showTickMarks="true" snapToTicks="true" value="5.0" GridPane.rowIndex="1"/>
            <TextField fx:id="rangeText" editable="false" GridPane.rowIndex="2"/>
            <ComboBox fx:id="chartType" prefWidth="150.0" promptText="Chart type" GridPane.columnIndex="1"
